<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd"
    
    >

	<context:annotation-config/>
	
	<bean id="line" class="beans.b12PostProcessor.Line"/>
	
	<bean id="pointA" class="beans.b12PostProcessor.Point">
		<property name="x" value="0"/>
		<property name="y" value="0"/>
	</bean>	
	
	<bean id="pointB" class="beans.b12PostProcessor.Point">
		<property name="x" value="100"/>
		<property name="y" value="50"/>
	</bean>	
	
	<bean class="beans.b12PostProcessor.DemoBeanPostProcessor" />
</beans>